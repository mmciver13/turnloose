}
.footer-logo{
  position: absolute;
  left: 50%;
  img { 
    max-width: 120px; 
    max-height: 80px; 
  }
}
.footer-logo img{
  z-index: 100;
  position: relative;
}
.site-footer {
  padding-top: $gutter*2;
  padding-bottom: $gutter;
}
.lt-ie9 .site-footer{
  border-top: $gutter*2 solid $colorFooterBg;
}
.footer-quicklinks li{ 
