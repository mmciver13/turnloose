.home__subtitle{
  @extend .uppercase;
  text-align: center;
  font-weight: bold;
  opacity: 0.75;
  font-size: 1em;
  padding-bottom: $gutter/2;
  margin-bottom: $gutter;
  position: relative;
  &::after{
    content: "";
    margin: auto;
    width: 30px;
    height: 3px;
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    background: black;
    opacity: 0.2;
  }
}


/*================ Colors ================*/
.homepage--white{
  background-color: #fff;
}
.homepage--light{
  background-color: {{ settings.homepage_background_light }};
}
.homepage--splash{
  background-color: $colorSplash;
}
.homepage--dark{
  background-color: {{ settings.homepage_background_dark }};
}
.homepage-product .product-page{
  background-color: transparent;
}
.homepage--splash, 
.homepage--dark{
  color: #fff;
  .on-sale,
  .figcaption a,
  .js-qty__adjust, 
  .ajaxcart__qty-adjust,
  .js-qty input[type="text"], 
  .figcaption a .price_wrapper,
  .ajaxcart__qty input[type="text"],
  input, textarea, button, select{
    color: #fff;
  }
  .on-sale .compare-at{
    color: rgba(#fff, 0.5);
  }  
  .btn--clear{
    color: white;
    border: 1px solid white;
  }
  .btn--clear.disabled {
    border: 1px solid rgba(#000, 0.3);
    color: rgba(#fff, 0.3);
  }
  .blog__article{
    background-color: #fff;
    color: $colorTextBody;
    border: none;
  }
}

.homepage--splash .blog__article .btn--splash{
  background-color: darken($colorSplash, 10%);
  &:hover{
    background-color: darken($colorSplash, 15%);
  }
}


/*================ Page ================*/
.homepage-page{
  hr{ 
    width: 75%; 
    margin-left: 12%;
  }
}
.homepage-page__content{
  text-align: center;
  font-size: 1.2em;
}

/*================ Logos ================*/
ul.homepage-logos__list{
  margin: $gutter/3 0 0 0;
  img{
    max-width: 150px;
    max-height: 20px;
  }
  li{
    margin: $gutter/3;
  }
  @include at-query($min, $small){
    margin: $gutter/2.5 0 0 0;
    img{
      max-width: 225px;
      max-height: 30px;
    }
    li{
      margin: $gutter/3;
    }
  }
  @include at-query($min, $medium){
    margin: $gutter/2 0 0 0;
    img{
      max-width: 300px;
      max-height: 40px;
    }
    li{
      margin: $gutter/3 $gutter;
    }
  }
}


/*================ Instagram ================*/
.homepage-instagram{
  margin-left: -5px;
  .grid__item{
    padding-left: 5px;
    margin-bottom: 5px;
  }
  a{ 
    position: relative;    
    color: white;
    text-align: center;
    font-size: 2em;
  }
  a:hover::after{
    content:'';
    background-color: black;
    position: absolute;
    opacity: 0.5;
    top: 0;
    left: 5px;
    right: 0;
    bottom: 0;
    z-index: 100;
  }
  .icon{
    position: absolute;
    z-index: 200;
    bottom: 5px;
    width: 27px;
    height: 27px;
    left: 50%;
    margin-left: -12px;
    opacity: 0;
    @include transition(all .2s ease-out);
  }
  a:hover .icon{
    opacity: 1;
    bottom: 47%;
  }
  a img{
    display: block;
  }
}

/*================ Custom ================*/
.mega-button{
  @include at-query($min, $small){ margin: $gutter*8 auto; }
  @include at-query($max, $small){ margin: $gutter*4 auto; }
  display: inline-block;
  background-color: $colorBody; 
  padding: 0;
  &:hover{
    color: $colorSplash;
  }
}
.mega-button-content{
  border: 1px solid $colorPrimary;
  display: inline-block;
  padding: 15px;
  margin: 8px;
  h1, h2, h3, h4, h5, h6, p{ margin-bottom: 0; }
}
.homepage-custom__parallax{
  overflow: hidden;
  position: relative;
  @include clearfix;
  &::before {
    content: '';
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background-color: $colorSplash;
    background-image: url('{{ 'homepage_lower_image.jpg' | asset_url }}');
    background-repeat: no-repeat;
    background-position: center center;
    background-size: cover;
    z-index: -1;
    -moz-box-shadow:    inset 0 0 15px rgba(#000000, 0.5);
    -webkit-box-shadow: inset 0 0 15px rgba(#000000, 0.5);
    box-shadow:         inset 0 0 15px rgba(#000000, 0.5);
    @include at-query($min, $small){
      will-change: transform;
      position: fixed;
    }
    @include at-query($max, $small){
      will-change: transform;
      position: absolute;
    }
  }
}

/*================ Hero ================*/
.homepage-hero{
  position: relative;
  overflow: hidden;
  background-color: #fff;
}
.homepage-hero-video, 
.homepage-hero-image{
  display: block;
  background-size: cover;
  background-position: center center;
  width: 100%;
  height: 100%;
  overflow: hidden;
  position: absolute;
  top: 0;
  right: 0;
  z-index: 1;
  video, iframe{
    min-width: 100%;
    min-height: 100%;
  }
}
.homepage-hero-video{
  background-color: #000;
}
.homepage-hero-video__close{
  position: absolute;
  z-index: 100;
  top: 10px;
  left: 10px;
  border-color: #fff;
  font-weight: 900;
  color: #fff;
  border-radius: 50px;
  height: 32px;
  width: 32px;
}
.homepage-hero-video__play{
  opacity: 1;
  @include transition(opacity 0.2s ease-out);
  &:hover{
    opacity: 0.75;
  }
}
.homepage-hero__cta-image{
  max-width: 33.33%;

  @include at-query($max, $medium){
    max-width: 50%;
  }
  @include at-query($max, $small){
    max-width: 80%;
  }
}

.homepage-hero-content{
  margin-top: {{settings.homepage_hero_height}};
  margin-bottom: {{settings.homepage_hero_height}};
  position:relative;
  z-index: 500;
  color: $colorBody;
  .homepage-hero-content__title{
    @extend .title;
    margin-bottom: $gutter/2;
  }
  .homepage-hero-content__block{
    margin-bottom:  $gutter;
  }
}
.hero-image-overlay{
  background-color: $colorTextBody;
  opacity: {{ settings.hero_overlay_opacity }};
  position: absolute;
  top:0;
  bottom:0;
  left:0;
  right:0;
  z-index: 1;
}
/*=== Hero Scroll ==*/
.scroll_icon_wrap {
  text-align: center;
  position: absolute;
  margin-left: auto;
  margin-right: auto;
  left: 0;
  right: 0;
  bottom: $gutter;
  z-index: 400;
  @include at-query($max, $small){
    display: none;
  }
}
.scroll_icon {
  display: inline-block;
  width: $gutter*1.5;
  height: $gutter*1.5;
  border-radius: 100px;
  text-align: center;
  position: relative;
  z-index: 1;
  color: $colorBody;
  text-decoration: none;
}
.scroll_icon{
  background: rgba(255,255,255,0.25);
}
.scroll_icon:hover {
  background: rgba(255,255,255,1);
  color: $colorSplash;
}
.homepage-hero #scroll{
  position: absolute;
  bottom: 0;
}

/* progressive enhancement for older broswers */
.supports-csstransforms {
  .scroll_icon{
    font-size: 0px;
    -webkit-transition: background 0.2s, color 0.2s;
    -moz-transition: background 0.2s, color 0.2s;
    transition: background 0.2s, color 0.2s;
  }
  .scroll_icon:after {
    pointer-events: none;
    position: absolute;
    width: 100%;
    height: 100%;
    border-radius: 50%;
    content: '';
    -webkit-box-sizing: content-box; 
    -moz-box-sizing: content-box; 
    box-sizing: content-box;
  }
  .scroll_icon:before {
    font-size: $gutter;
    display: block;
    line-height: 1.5 !important;
  }
  .scroll_icon:hover:after {
    -webkit-transform: scale(1);
    -moz-transform: scale(1);
    -ms-transform: scale(1);
    transform: scale(1);
    opacity: 1;
  }
  .scroll_icon:after {
    top: -7px;
    left: -7px;
    padding: 7px;
    box-shadow: 0 0 0 4px #fff;
    -webkit-transition: -webkit-transform 0.2s, opacity 0.2s;
    -webkit-transform: scale(.8);
    -moz-transition: -moz-transform 0.2s, opacity 0.2s;
    -moz-transform: scale(.8);
    -ms-transform: scale(.8);
    transition: transform 0.2s, opacity 0.2s;
    transform: scale(.8);
    opacity: 0;
  }
}


