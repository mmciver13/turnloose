/*================ Horizontal Rules ================*/
hr{
  clear: both;
  border-top: solid $colorBorder;
  border-width: 1px 0 0;
  margin: $gutter 0;
  height: 0;

  &.hr--small {
    margin: ($gutter / 2) 0;
  }
  &.hr--double {
    margin: ($gutter * 2) 0;
  }
  &.hr--clear {
    border-top-color: transparent;
  }
  &.hr--dash {
    margin: $gutter auto;
    width: 80px;
  }
  &.hr--thick {
    border-width: 2px 0 0;
  }
  &.hr--underline{
    margin-top: 10px;
    margin-bottom: 20px;
  }
}

